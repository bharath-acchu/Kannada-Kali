<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kkali.kannadakali">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Alphabets"
            android:label="@string/alphabets" />
        <activity
            android:name=".Numbers"
            android:label="@string/numbers" />
        <activity android:name=".Family"
            android:label="@string/family"/>
        <activity android:name=".Convo"
            android:label="@string/convo"/>
        <activity android:name=".Phrases"
            android:label="@string/phrases"/>
        <activity android:name=".Words"
            android:label="@string/words"></activity>
    </application>

</manifest>